{2.1}
http.HandleFunc("/", pro2_1.SayHellName)       	//设置访问的路由
http.HandleFunc("/login", pro2_1.Login)         //设置访问的路由
err := http.ListenAndServe(":9000", nil) 	//设置监听的端口
if err != nil {
    log.Fatal("ListenAndServe: ", err)
}

